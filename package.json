{
  "name": "@murali-codes/vue-basic-components",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "pre-commit": "^1.2.2",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/test-utils": "^2.0.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.2.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vitest": "^0.18.1",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.38.4"
  },
  "description": "This template should help get you started developing with Vue 3 and TypeScript in Vite. The template uses Vue 3 `<script setup>` SFCs, check out the [script setup docs](https://v3.vuejs.org/api/sfc-script-setup.html#sfc-script-setup) to learn more.",
  "main": "src/main.ts",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Murali-codes/vue-basic-components.git"
  },
  "keywords": [
    "vue3",
    "basic-components",
    "vue3-ts",
    "vite"
  ],
  "author": "Murali Krishna Gundoji",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Murali-codes/vue-basic-components/issues"
  },
  "homepage": "https://github.com/Murali-codes/vue-basic-components#readme"
}
